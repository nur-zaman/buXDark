{
  "name": "buXDark",
  "version": "0.0.2",
  "description": "Dark theme for buX",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "buXDark.png"
  },
  "icons": { "16": "icon/16.png",
    "48": "icon/48.png",
   "128": "icon/128.png" },
  "permissions": [
    "storage",
    "activeTab"
  ],

  "content_scripts": [
    {
      "css": [
        "styles.css"
      ],
      "js": [
        "script.js"
      ],
      "matches": [
        "https://bux.bracu.ac.bd/*"
      ]
    }
  ],
  "manifest_version": 2
}